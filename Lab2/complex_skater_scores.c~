#include <stdlib.h>
#include <stdio.h>

#define ARRAY_SIZE 10

int calculate_mean(int n, double judge[]){

    double total = 0.0, mean;
    int i;
    
    for(i=0; i < n; i++){
        total += judge[i];
    }
    mean = total/n;

    return mean;
}

